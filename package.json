      {
        "command": "gistpad.copyGistPadUrl",
        "title": "Copy GistPad URL",
        "category": "GistPad"
      },
      {
        "command": "gistpad.copyGistUrl",
        "title": "Copy GitHub URL",
        "category": "GistPad"
      },
      {
        "command": "gistpad.deleteFile",
